SELECT COUNT(商家id) FROM 商家

SELECT * FROM 图书
SELECT 作者 FROM 图书
SELECT * FROM 图书 WHERE 类型 = '小说'
SELECT 书名id, 售价 FROM 图书与平台

SELECT 公司名 FROM 餐饮公司 INNER JOIN 餐饮公司城市消费 ON 餐饮公司.公司id = 餐饮公司城市消费.公司id WHERE 人均消费额 < 500
SELECT 所属集团 FROM 餐饮公司 INNER JOIN 餐饮公司城市消费 ON 餐饮公司.公司id = 餐饮公司城市消费.公司id WHERE 人均消费额 < 500
SELECT 经营年数 FROM 餐饮公司 INNER JOIN 餐饮公司城市消费 ON 餐饮公司.公司id = 餐饮公司城市消费.公司id WHERE 人均消费额 < 500
SELECT 名称 FROM 城市 INNER JOIN 餐饮公司 ON 城市.城市id = 餐饮公司.总部城市id INNER JOIN 餐饮公司城市消费 ON 餐饮公司.公司id = 餐饮公司城市消费.公司id WHERE 人均消费额 < 500

SELECT "中文队名" FROM "球队" WHERE "中文队名" LIKE '%公牛%'
SELECT "夺冠次数" FROM "球队" WHERE "中文队名" LIKE '%公牛%'
SELECT "赛区", "所属联盟" FROM "球队" WHERE "中文队名" LIKE '%公牛%'
SELECT "总教练" FROM "NBA总冠军" JOIN "球队" ON "NBA总冠军"."球队id"="球队"."球队id" WHERE "球队"."中文队名" LIKE '%公牛%'
SELECT "赛季" FROM "NBA总冠军" JOIN "球队" ON "NBA总冠军"."球队id"="球队"."球队id" WHERE "球队"."中文队名" LIKE '%公牛%'; SELECT "赛季" FROM "MVP记录" JOIN "球队" ON "MVP记录"."所属球队"="球队"."球队id" WHERE "球队"."中文队名" LIKE '%公牛%'

SELECT 国籍 FROM 篮球运动员 WHERE 中文名 = '库里'
SELECT 参加选秀年, 是否状元 FROM 篮球运动员 WHERE 中文名 = '库里'
SELECT 获得球队 FROM 篮球运动员 WHERE 中文名 = '库里'

SELECT "国籍","毕业院校" FROM "篮球运动员" WHERE "中文名"='杜兰特'
SELECT "赛季" FROM "MVP记录" WHERE "球员id"='杜兰特'
