SELECT 型号id, COUNT(*) AS 背光灯寿命 FROM 电视机型号

SELECT * FROM 羽毛球球员 JOIN 重要赛事 ON 羽毛球球员.球员id = 重要赛事.球员id JOIN 奥运会冠亚军 ON 羽毛球球员.球员id = 奥运会冠亚军.金牌获得者id JOIN 冠军国家 ON 羽毛球球员.球员id = 冠军国家.国家名称 JOIN 球员夺冠次数 ON 奥运会冠亚军.赛事id = 球员夺冠次数.赛事id WHERE 李敬元 = '李敬元'
SELECT 奥运会名称, 项目名称, 金牌获得者id, 银牌获得者id, 铜牌获得者id FROM 奥运会冠亚军 WHERE 金牌获得者id IN ( SELECT 金牌获得者id FROM 羽毛球球员 JOIN 重要赛事 ON 羽毛球球员.球员id = 重要赛事.球员id JOIN 奥运会冠亚军 ON 羽毛球球员.球员id = 奥运会冠亚军.金牌获得者id JOIN 冠军国家 ON 羽毛球球员.球员id = 冠军国家.国家名称 JOIN 球员夺冠次数 ON 奥运会冠亚军.赛事id = 球员夺冠次数.赛事id )
SELECT 奥运会名称, 项目名称, 金牌获得者id, 银牌获得者id, 铜牌获得者id FROM 奥运会冠亚军 WHERE 银牌获得者id IN ( SELECT 银牌获得者id FROM 羽毛球球员 JOIN 重要赛事 ON 羽毛球球员.球员id = 重要赛事.球员id JOIN 奥运会冠亚军 ON 羽毛球球员.球员id = 奥运会冠亚军.银牌获得者id JOIN 冠军国家 ON 羽毛球球员.球员id = 冠军国家.国家名称 JOIN 球员夺冠次数 ON 奥运会冠亚军.赛事id = 球员夺冠次数.赛事id )

SELECT 国家名称 FROM 冠军国家 WHERE 赛事id IN ( SELECT 赛事id FROM 重要赛事 WHERE 赛事名称 = '苏迪曼杯' )
SELECT 球员id FROM 奥运会冠亚军 WHERE 赛事id IN ( SELECT 赛事id FROM 重要赛事 WHERE 赛事名称 = '苏迪曼杯' )
SELECT 球员姓名 FROM 羽毛球球员 WHERE 球员姓名 NOT IN ( SELECT 球员姓名 FROM 羽毛球球员 WHERE 性别 = '男' AND 国籍 = '中国' )
