SELECT 比赛.name, 举办单位.name, 单位性质.name FROM 比赛 JOIN 参赛队伍 ON 比赛.比赛id = 参赛队伍.比赛id JOIN 参赛学生 ON 比赛.参赛id = 参赛学生.参赛id JOIN 学生参赛记录 ON 参赛队伍.加入队伍id = 学生参赛记录.加入队伍id JOIN 单位 ON 参赛学生.学校id = 单位.学校id WHERE 比赛.比赛id = ? LIMIT 1
