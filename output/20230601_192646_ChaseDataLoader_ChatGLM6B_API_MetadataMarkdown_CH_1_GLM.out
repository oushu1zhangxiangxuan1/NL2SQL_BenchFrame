SELECT t1.医院名, t1.科室数量, t1.重点专科数量 FROM "医院" t1 INNER JOIN "医院排名" t2 ON t1.医院名 = t2.医院名 AND t1.所属城市id = t2.城市id INNER JOIN "城市" t3 ON t2.城市名 = t3.名称 AND t2.所属省份 = t3.所属省份 INNER JOIN "特色科室" t4 ON t3.城市id = t4.医院id AND t4.是否重点 = t4.是否重点 WHERE t1.重点专科数量 = ( SELECT COUNT(*) FROM "特色科室" WHERE 医院名 = t1.医院名 ) ORDER BY t1.重点专科数量 DESC LIMIT 1
SELECT COUNT(*) as 数量 FROM "医院" t1 INNER JOIN "医院排名" t2 ON t1.医院名 = t2.医院名 AND t1.所属城市id = t2.城市id INNER JOIN "城市" t3 ON t2.城市名 = t3.名称 AND t2.所属省份 = t3.所属省份 INNER JOIN "特色科室" t4 ON t3.城市id = t4.医院id AND t4.是否重点 = t4.是否重点 WHERE t1.重点专科数量 = ( SELECT COUNT(*) FROM "特色科室" WHERE 医院名 = t1.医院名 ) ORDER BY 数量 DESC LIMIT 1
